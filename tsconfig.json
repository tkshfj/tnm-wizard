{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "outDir": "static",
    "rootDir": "static-src"
  },
  "include": ["static-src/**/*.ts"]
}
